            <origin xyz="0.072 -0.005 0.001" rpy="0.000 -0.010 0.010" />
            <parent link="frontright" />
            <child link="frontright_fisheye" />
        </joint>

        <!-- Left camera -->
        <link name="left"/>
        <joint name="left_joint" type="fixed">
            <origin xyz="-0.164 0.109 0.037" rpy="-1.839 0.005 0.006" />
